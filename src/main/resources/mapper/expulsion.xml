<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="site.metacoding.red.domain.expulsion.ExpulsionDAO">
	
	<insert id="insert">
		INSERT INTO expulsion(teamId,positionsId,playerId,reason,expDate)
		VALUES(#{teamId}, #{positionsId}, #{playerId}, #{reason},
		now())
	</insert>

	<select id="findById" resultType="site.metacoding.red.domain.expulsion.ExpulsionDAO">
		SELECT * FROM expulsion WHERE id = #{id}
	</select>

</mapper>