<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="site.metacoding.red.domain.stadium.StadiumDao">
	
	<insert id="insert">
		INSERT INTO stadium(stadiumName,openDate)
		VALUES(#{stadiumName},now())
	</insert>

	<select id="findById" resultType="site.metacoding.red.domain.stadium.StadiumDao">
		SELECT * FROM stadium WHERE id = #{id}
	</select>

</mapper>